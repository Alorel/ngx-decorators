{
  "name": "ngx-decorate",
  "version": "1.1.1",
  "description": "Useful decorators for Angular 2 and above",
  "main": "dist/index.js",
  "browser": "dist/ngx-decorators.umd.js",
  "jsdelivr": "dist/ngx-decorators.umd.js",
  "esm5": "dist/esm5/index.js",
  "fesm5": "dist/esm5/index.js",
  "module": "dist/esm5/index.js",
  "esm2015": "dist/esm2015/index.js",
  "fesm2015": "dist/esm2015/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "angular",
    "angular 2",
    "angular 4",
    "angular 5",
    "angular 6",
    "decorator",
    "decorators",
    "ngx",
    "util",
    "utility",
    "utilities"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "concurrently \"npm run build:cjs\" \"npm run build:umd\" \"npm run build:esm2015\" \"npm run build:esm5\"",
    "build:cjs": "tsc -p ./tsconfig-cjs.json",
    "build:esm2015": "tsc -p ./tsconfig-esm2015.json",
    "build:esm5": "tsc -p ./tsconfig-esm5.json",
    "build:umd": "webpack",
    "postbuild:cjs": "node build/clean-empty-output.js",
    "pretest": "rimraf coverage",
    "test": "nyc mocha --opts ./mocha.opts",
    "test:watch": "npm run test -- --watch",
    "tslint": "tslint -s ./node_modules/custom-tslint-formatters/formatters -t grouped -p tsconfig.test.json",
    "tslint:fix": "npm run tslint -- --fix",
    "typecheck": "tsc --noEmit -p ./tsconfig-esm2015.json",
    "typecheck:watch": "npm run typecheck -- --watch",
    "doctoc": "doctoc README.md --github",
    "pretsdoc": "rimraf docs",
    "tsdoc": "typedoc --out docs dist --mode file --includeDeclarations --excludeExternals --hideGenerator --name \"ngx-decorate Documentation\" --readme README.md",
    "posttsdoc": "node build/minify-docs.js",
    "reinstall:yarn": "rm -rf node_modules yarn.lock package-lock.json && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alorel/ngx-decorators.git"
  },
  "author": "Alorel <a.molcanovas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alorel/ngx-decorators/issues"
  },
  "homepage": "https://alorel.github.io/ngx-decorators/",
  "devDependencies": {
    "@alorel-personal/conventional-changelog-alorel": "2.0.2",
    "@alorel-personal/semantic-release": "1.2.2",
    "@alorel-personal/tslint-rules": "1.2.2",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.1",
    "@semantic-release/github": "^5.0.2",
    "@semantic-release/npm": "^5.0.1",
    "@types/bluebird": "3.5.21",
    "@types/chai": "^4.1.4",
    "@types/cross-spawn": "^6.0.0",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^5.0.35",
    "@types/mocha": "^5.2.5",
    "@types/rimraf": "^2.0.2",
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "concurrently": "^3.6.1",
    "coveralls": "^3.0.2",
    "cross-spawn": "^6.0.5",
    "cssmin": "^0.4.3",
    "custom-tslint-formatters": "^2.4.0",
    "doctoc": "^1.3.1",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.2",
    "htmlmin": "^0.0.7",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.6",
    "source-map-support": "^0.5.6",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "typedoc": "^0.11.1",
    "typescript": "^3.0.1",
    "typescript-proto-decorator": "^2.0.1",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0"
  },
  "greenkeeper": {
    "ignore": [
      "@types/bluebird"
    ]
  }
}
