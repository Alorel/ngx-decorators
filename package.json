{
  "name": "ngx-decorate",
  "version": "3.0.3",
  "description": "Useful decorators for Angular 2 and above",
  "main": "index.js",
  "browser": "_bundle/umd.js",
  "jsdelivr": "_bundle/umd.min.js",
  "esm5": "_bundle/esm5/index.js",
  "fesm5": "_bundle/fesm5.js",
  "module": "_bundle/fesm5.js",
  "esm2015": "_bundle/esm2015/index.js",
  "fesm2015": "_bundle/fesm2015.js",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "keywords": [
    "angular",
    "angular 2",
    "angular 4",
    "angular 5",
    "angular 6",
    "decorator",
    "decorators",
    "ngx",
    "util",
    "utility",
    "utilities"
  ],
  "scripts": {
    "build": "alo build",
    "pretest": "rimraf coverage",
    "test": "nyc mocha --opts ./mocha.opts",
    "test:watch": "npm --scripts-prepend-node-path=auto run test -- --watch",
    "tslint": "alo tslint -p tsconfig.test.json",
    "tslint:fix": "npm --scripts-prepend-node-path=auto run tslint -- --fix",
    "typecheck": "tsc --noEmit -p ./tsconfig.json",
    "typecheck:watch": "npm --scripts-prepend-node-path=auto run typecheck -- --watch",
    "doctoc": "doctoc README.md --github",
    "pretsdoc": "rimraf docs",
    "tsdoc": "typedoc --out docs dist --mode file --includeDeclarations --excludeExternals --hideGenerator --name \"ngx-decorate Documentation\" --readme README.md",
    "posttsdoc": "node build/minify-docs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alorel/ngx-decorators.git"
  },
  "author": "Alorel <a.molcanovas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alorel/ngx-decorators/issues"
  },
  "homepage": "https://alorel.github.io/ngx-decorators/",
  "peerDependencies": {
    "tslib": "^1.6.0"
  },
  "devDependencies": {
    "@alorel-personal/build-tools": "4.7.17",
    "@alorel-personal/conventional-changelog-alorel": "2.1.1",
    "@alorel-personal/semantic-release": "1.4.0",
    "@alorel-personal/tslint-rules": "5.0.5",
    "@semantic-release/changelog": "~3.0.0",
    "@semantic-release/exec": "~3.3.0",
    "@semantic-release/git": "~7.0.3",
    "@types/bluebird": "~3.5.24",
    "@types/chai": "~4.2.0",
    "@types/cross-spawn": "~6.0.0",
    "@types/fs-extra": "~8.0.0",
    "@types/glob": "~7.1.0",
    "@types/lodash": "~4.14.116",
    "@types/mocha": "~5.2.5",
    "@types/rimraf": "~2.0.2",
    "bluebird": "~3.7.0",
    "chai": "~4.2.0",
    "coveralls": "~3.0.2",
    "cross-spawn": "~7.0.0",
    "cssmin": "~0.4.3",
    "doctoc": "~1.4.0",
    "fs-extra": "~8.1.0",
    "glob": "~7.1.3",
    "htmlmin": "~0.0.7",
    "lodash": "~4.17.14",
    "mocha": "~6.2.0",
    "nyc": "~14.1.0",
    "rimraf": "~3.0.0",
    "semantic-release": "^15.10.6",
    "source-map-support": "~0.5.9",
    "ts-node": "^8.0.2",
    "tslib": "~1.10.0",
    "typedoc": "~0.15.0",
    "typescript-proto-decorator": "~2.1.0"
  }
}
